        <ObjectProperty abbreviatedIRI="Governance:responsibleFor"/>
        <NamedIndividual abbreviatedIRI="Governance:Development_1"/>
        <NamedIndividual abbreviatedIRI="Governance:Reporting_And_Analysis_System_1"/>
