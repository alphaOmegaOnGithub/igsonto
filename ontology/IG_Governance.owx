    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <AbbreviatedIRI>Governance:Give_Articles_Of_Chapter_That_Include_These_Articles</AbbreviatedIRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Returns all articles of a certain GDPR chapter that includes the user 
articles

User Input: ?inputArticle

PREFIX rdf: &lt;http://www.w3.org/1999/02/22-rdf-syntax-ns#&gt;
PREFIX owl: &lt;http://www.w3.org/2002/07/owl#&gt;
PREFIX xsd: &lt;http://www.w3.org/2001/XMLSchema#&gt;
PREFIX rdfs: &lt;http://www.w3.org/2000/01/rdf-schema#&gt;
PREFIX igonto: &lt;http://www.semanticweb.org/IG/Governance#&gt;

SELECT DISTINCT ?articles ?chapter WHERE {
  ?inputArticle igonto:definedInChapter ?chapter. 
  ?articles  igonto:definedInChapter ?chapter. 
  FILTER (?inputArticle IN (igonto:Article_15, igonto:Article_25, igonto:Article_17))
}</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <AbbreviatedIRI>Governance:Give_Articles_That_Include_All_Keywords</AbbreviatedIRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Returns articles that inlcude all keywords the user provided.

User Input: ?filterKeyword, (COUNT(DISTINCT ?filterKeyword) = ?)

PREFIX rdf: &lt;http://www.w3.org/1999/02/22-rdf-syntax-ns#&gt;
PREFIX owl: &lt;http://www.w3.org/2002/07/owl#&gt;
PREFIX xsd: &lt;http://www.w3.org/2001/XMLSchema#&gt;
PREFIX rdfs: &lt;http://www.w3.org/2000/01/rdf-schema#&gt;
PREFIX igonto: &lt;http://www.semanticweb.org/IG/Governance#&gt;

SELECT DISTINCT ?article
WHERE {
  ?article igonto:includesKeyword ?keyword.
  
  FILTER (
    ?keyword IN (igonto:Right, igonto:Data_Subject)
  )
  {
    SELECT ?article
    WHERE {
      ?article igonto:includesKeyword ?filterKeyword.
      FILTER (?filterKeyword IN (igonto:Right, igonto:Data_Subject))
    }
    GROUP BY ?article
    HAVING (COUNT(DISTINCT ?filterKeyword) = 2)  # Adjust this number to match the number of required keywords
  }
}</Literal>
    </AnnotationAssertion>
