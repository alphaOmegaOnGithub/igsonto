#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/igonto/information#Asset
:Asset rdf:type owl:Class ;
       rdfs:subClassOf :Data ;
       :classDescription "Represents a data container that links digital content, metadata and their information together." ;
       rdfs:label "Asset" .


###  http://www.semanticweb.org/igonto/information#Audit_Data
:Audit_Data rdf:type owl:Class ;
            rdfs:subClassOf :Sensitive_Data ;
            :classDescription "Captures information related to audit trails and data activities and therfore critical and sensitive data." ;
            rdfs:label "Audit Data" .


###  http://www.semanticweb.org/igonto/information#Audit_Trail
:Audit_Trail rdf:type owl:Class ;
             rdfs:subClassOf :Record_Control_Structure ;
             :classDescription "\"Represents a systematic recording of record activities, including the capture of their access, modifications and other action information." ;
             rdfs:label "Audit Trail" .


###  http://www.semanticweb.org/igonto/information#Category
:Category rdf:type owl:Class ;
          rdfs:subClassOf :Record_Control_Structure ;
          :classDescription "Signifies the classification and categorization of Records." ;
          rdfs:label "Category" .


###  http://www.semanticweb.org/igonto/information#Container
:Container rdf:type owl:Class ;
           rdfs:subClassOf :Record ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :containerContainsFile ;
                             owl:someValuesFrom :File
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :containsFile ;
                             owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass :File
                           ] ;
           :classDescription "Represents a logical or physical entity to organize and store Data." ;
           rdfs:label "Container" .


###  http://www.semanticweb.org/igonto/information#Content
:Content rdf:type owl:Class ;
         rdfs:subClassOf :Data ;
         :classDescription "Represents valuable and concrete information of Data." ;
         rdfs:label "Content" .


###  http://www.semanticweb.org/igonto/information#Control_Data
:Control_Data rdf:type owl:Class ;
              rdfs:subClassOf :Record_Control_Structure ;
              rdfs:label "Control Data" .


###  http://www.semanticweb.org/igonto/information#Cost
:Cost rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :costIsPartOfData ;
                        owl:someValuesFrom :Data
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :producedByData ;
                        owl:someValuesFrom :Data
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :value ;
                        owl:someValuesFrom xsd:string
                      ] ;
      :classDescription "Represents the fincancial aspect of managing information assets. Cost is directly connected to the concept of Data." ;
      rdfs:label "Cost" .


###  http://www.semanticweb.org/igonto/information#Data
:Data rdf:type owl:Class ;
      owl:equivalentClass :Item ,
                          [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                   owl:onProperty :hasComponent ;
                                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                   owl:onClass :Cost
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :hasComponent ;
                                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                   owl:onClass :Risk
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :hasComponent ;
                                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                   owl:onClass :Value
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :classifiedByRecord ;
                        owl:someValuesFrom :Record
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :usedOnRecord ;
                        owl:someValuesFrom :Record
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :containsValue ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Value
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :exposesRisk ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Risk
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :producesCost ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Cost
                      ] ;
      :classDescription "Superclass of Data seen in the context of information value." ;
      rdfs:label "Data" .


###  http://www.semanticweb.org/igonto/information#Data_Dictionary
:Data_Dictionary rdf:type owl:Class ;
                 rdfs:subClassOf [ rdf:type owl:Restriction ;
                                   owl:onProperty :vocabularyFor ;
                                   owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass :Taxonomy
                                 ] ;
                 :classDescription "Represents the Data Dictionary of an enterprise that serves as vocabular for its Taxonomy." ;
                 rdfs:label "Data Dictionary" .


###  http://www.semanticweb.org/igonto/information#Document
:Document rdf:type owl:Class ;
          rdfs:subClassOf :Data ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasEdition ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :Edition
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasRendition ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :Rendition
                          ] ;
          :classDescription "Represents a structured representation of information that embodies specific versions and alternative presentations of a document with different formats, layouts, etc.." .


###  http://www.semanticweb.org/igonto/information#Edition
:Edition rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :editionIsPartOfDocument ;
                           owl:someValuesFrom :Document
                         ] ;
         :classDescription "Represents the versioning of information. Instances form this class capture changes, updates or revisions of Data." ;
         rdfs:label "Edition" .


###  http://www.semanticweb.org/igonto/information#File
:File rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :containedInContainer ;
                        owl:someValuesFrom :Container
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :containedInFolder ;
                        owl:someValuesFrom :Folder
                      ] ;
      :classDescription "Represents stored information in the form of digital or physical files." ;
      rdfs:label "File" .


###  http://www.semanticweb.org/igonto/information#Folder
:Folder rdf:type owl:Class ;
        rdfs:subClassOf :Container ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :folderContainsFile ;
                          owl:someValuesFrom :File
                        ] ;
        :classDescription "Represents a digital hierarchical organization unit to structure Documents and Data." ;
        rdfs:label "Folder" .


###  http://www.semanticweb.org/igonto/information#Goal
:Goal rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :goalIsPartOfPolicy ;
                        owl:someValuesFrom :Policy
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :scope ;
                        owl:someValuesFrom xsd:string
                      ] ;
      :classDescription "Represents desired outcomes of the enterprise policy but put into context of information." ;
      rdfs:label "Goal" .


###  http://www.semanticweb.org/igonto/information#Hold
:Hold rdf:type owl:Class ;
      rdfs:subClassOf :Record_Control_Structure ;
      :classDescription "Represents the hold process for the temporary suspension of record management processes and activities." ;
      rdfs:label "Hold" .


###  http://www.semanticweb.org/igonto/information#Information
:Information rdf:type owl:Class ;
