                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty :usesSchedule ;
                                            owl:someValuesFrom :Schedule
                                          ] ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty :endDate ;
                                            owl:someValuesFrom xsd:dateTime
                                          ] ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty :startDate ;
                                            owl:someValuesFrom xsd:dateTime
                                          ] ;
                          :classDescription "Represents processes and mechanisms for creating, maintaining and disposing of Records." ;
                          rdfs:label "Record Control Structure" .


###  http://www.semanticweb.org/igonto/information#Rendition
:Rendition rdf:type owl:Class ;
