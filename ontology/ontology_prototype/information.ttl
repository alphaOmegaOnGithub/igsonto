#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/igonto/information#appliesRule
:appliesRule rdf:type owl:ObjectProperty ;
             rdfs:domain :Policy ;
             rdfs:range :Rule .


###  http://www.semanticweb.org/igonto/information#classifiesData
:classifiesData rdf:type owl:ObjectProperty ;
                rdfs:domain :Record ;
                rdfs:range :Data .


###  http://www.semanticweb.org/igonto/information#containsFile
:containsFile rdf:type owl:ObjectProperty ;
              rdfs:domain :Container ,
                          :Folder ;
              rdfs:range :File .


###  http://www.semanticweb.org/igonto/information#containsValue
:containsValue rdf:type owl:ObjectProperty ;
               rdfs:domain :Data ;
               rdfs:range :Value .


###  http://www.semanticweb.org/igonto/information#definesSchedule
:definesSchedule rdf:type owl:ObjectProperty ;
                 rdfs:domain :Rule ;
                 rdfs:range :Schedule .


###  http://www.semanticweb.org/igonto/information#employsRule
:employsRule rdf:type owl:ObjectProperty ;
             rdfs:domain :Policy ;
             rdfs:range :Rule .


###  http://www.semanticweb.org/igonto/information#exposesRisk
:exposesRisk rdf:type owl:ObjectProperty ;
             rdfs:domain :Data ;
             rdfs:range :Risk .


###  http://www.semanticweb.org/igonto/information#grantsPriviligeToRole
:grantsPriviligeToRole rdf:type owl:ObjectProperty ;
                       rdfs:domain :Policy ;
                       rdfs:range :Role .


###  http://www.semanticweb.org/igonto/information#hasComponent
:hasComponent rdf:type owl:ObjectProperty ;
              rdfs:domain :Data ,
                          :Policy ;
              rdfs:range :Cost ,
                         :Goal ,
                         :Metric ,
                         :Risk ,
                         :Rule ,
                         :Value .


###  http://www.semanticweb.org/igonto/information#hasEdition
:hasEdition rdf:type owl:ObjectProperty ;
            rdfs:domain :Document ;
            rdfs:range :Edition .


###  http://www.semanticweb.org/igonto/information#hasGoal
:hasGoal rdf:type owl:ObjectProperty ;
         rdfs:domain :Policy ;
         rdfs:range :Goal .


###  http://www.semanticweb.org/igonto/information#hasPrivilige
:hasPrivilige rdf:type owl:ObjectProperty ;
              rdfs:domain :Role ;
              rdfs:range :Privilige .


###  http://www.semanticweb.org/igonto/information#hasRecordControlStructure
:hasRecordControlStructure rdf:type owl:ObjectProperty ;
                           rdfs:domain :Record ;
                           rdfs:range :Record_Control_Structure .


###  http://www.semanticweb.org/igonto/information#hasRendition
:hasRendition rdf:type owl:ObjectProperty ;
              rdfs:domain :Document ;
              rdfs:range :Rendition .


###  http://www.semanticweb.org/igonto/information#hasVersion
:hasVersion rdf:type owl:ObjectProperty ;
            rdfs:domain :Item ;
            rdfs:range :Version .


###  http://www.semanticweb.org/igonto/information#monitorsMetric
:monitorsMetric rdf:type owl:ObjectProperty ;
                rdfs:domain :Policy ;
                rdfs:range :Metric .


###  http://www.semanticweb.org/igonto/information#producesCost
:producesCost rdf:type owl:ObjectProperty ;
              rdfs:domain :Data ;
              rdfs:range :Cost .


###  http://www.semanticweb.org/igonto/information#usedOnRecordControlStructure
:usedOnRecordControlStructure rdf:type owl:ObjectProperty ;
                              rdfs:domain :Schedule ;
                              rdfs:range :Record_Control_Structure .


###  http://www.semanticweb.org/igonto/information#usesDataDictionary
:usesDataDictionary rdf:type owl:ObjectProperty ;
                    rdfs:domain :Record ;
                    rdfs:range :Data_Dictionary .


###  http://www.semanticweb.org/igonto/information#vocabularyFor
:vocabularyFor rdf:type owl:ObjectProperty ;
               rdfs:domain :Data_Dictionary ;
               rdfs:range :Taxonomy .


