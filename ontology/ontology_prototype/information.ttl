###  http://www.semanticweb.org/igonto/information#Policy
:Policy rdf:type owl:Class ;
        owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                     owl:onProperty :hasComponent ;
                                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                     owl:onClass :Goal
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :hasComponent ;
                                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                     owl:onClass :Metric
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :hasComponent ;
                                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                     owl:onClass :Rule
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :appliesRule ;
                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Rule
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :employsRule ;
                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Rule
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :grantsPriviligeToRole ;
                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Role
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasGoal ;
                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Goal
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :monitorsMetric ;
                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Metric
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :scope ;
                          owl:someValuesFrom xsd:string
                        ] ;
        :classDescription "Reprsents authoritative guidelines for information withing an enteprise regarding its management, use and protection. It grants priviliges to Roles, applies Rules and try to acheve a specific Goal." ;
        rdfs:label "Policy" .


###  http://www.semanticweb.org/igonto/information#Privilige
:Privilige rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
