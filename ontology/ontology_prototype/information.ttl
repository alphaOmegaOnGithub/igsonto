###  http://www.semanticweb.org/igonto/information#Record
:Record rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :classifiesData ;
                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Data
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasRecordControlStructure ;
                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Record_Control_Structure
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :usesDataDictionary ;
                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Data_Dictionary
                        ] ;
        :classDescription "Represents processing records seen from an information persepctive. Every record has a Record Control Structure, uses a specific vocabulary from the Data Dictionary and classifies Data." ;
        rdfs:label "Record" .


###  http://www.semanticweb.org/igonto/information#Record_Control_Structure
:Record_Control_Structure rdf:type owl:Class ;
                          rdfs:subClassOf [ rdf:type owl:Restriction ;
                                            owl:onProperty :partOfRecord ;
                                            owl:someValuesFrom :Record
