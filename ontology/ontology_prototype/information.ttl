###  http://www.semanticweb.org/igonto/information#Risk
:Risk rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :exposedByData ;
                        owl:someValuesFrom :Data
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :riskIsPartOfData ;
                        owl:someValuesFrom :Data
                      ] ;
      :classDescription "Represents the concept of risk related to data in the context of security, compliance or reputational risks." ;
      rdfs:label "Risk" .


###  http://www.semanticweb.org/igonto/information#Role
:Role rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :priviligeGrantedByPolicy ;
                        owl:someValuesFrom :Policy
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasPrivilige ;
                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Privilige
                      ] ;
      :classDescription "Represents responisbilites and permissions assigned to individuals to access, manage or oversee information." ;
      rdfs:label "Role" .


###  http://www.semanticweb.org/igonto/information#Rule
:Rule rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :employedByPolicy ;
                        owl:someValuesFrom :Policy
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :ruleIsPartOfPolicy ;
                        owl:someValuesFrom :Policy
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :definesSchedule ;
                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Schedule
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :scope ;
                        owl:someValuesFrom xsd:string
                      ] ;
      :classDescription "Reprsents a specific guideline that is part of a policy to ensure compliance and regulation standards." ;
      rdfs:label "Rule" .


###  http://www.semanticweb.org/igonto/information#Schedule
:Schedule rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :definedByRule ;
                            owl:someValuesFrom :Rule
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :usedOnRecordControlStructure ;
                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :Record_Control_Structure
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :endDate ;
                            owl:someValuesFrom xsd:dateTime
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :startDate ;
                            owl:someValuesFrom xsd:dateTime
                          ] ;
          :classDescription "Represents predefined plans defined by Rules for managing the lifecycle of records." ;
          rdfs:label "Schedule" .


