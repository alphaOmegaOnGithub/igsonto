###  http://www.semanticweb.org/igonto/igonto#Large_Q1
igonto:Large_Q1 rdf:type owl:NamedIndividual ,
                         igonto:Large_Enterprise_Query ;
                rdfs:comment """@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh:    <http://www.w3.org/ns/shacl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix igonto:   <http://www.semanticweb.org/igonto/igonto#> .
@prefix organization:   <http://www.semanticweb.org/igonto/organization#> .
@prefix jurisdiction:   <http://www.semanticweb.org/igonto/jurisdiction#> .
@prefix lifecycle:   <http://www.semanticweb.org/igonto/lifecycle#> .
@prefix information:   <http://www.semanticweb.org/igonto/information#> .
@prefix enterprise:   <http://www.semanticweb.org/igonto/enterprise#> .


SELECT * WHERE{
  organization:Large_Enterprise_Instance jurisdiction:operatesIn ?country.
  ?country jurisdiction:isRegulatedBy ?regulation.
  ?country jurisdiction:belongsToJurisdiction ?jurisdiction.
}""" .


###  http://www.semanticweb.org/igonto/igonto#Large_Q2
igonto:Large_Q2 rdf:type owl:NamedIndividual ,
                         igonto:Large_Enterprise_Query ;
                rdfs:comment """@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh:    <http://www.w3.org/ns/shacl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix igonto:   <http://www.semanticweb.org/igonto/igonto#> .
@prefix organization:   <http://www.semanticweb.org/igonto/organization#> .
@prefix jurisdiction:   <http://www.semanticweb.org/igonto/jurisdiction#> .
@prefix lifecycle:   <http://www.semanticweb.org/igonto/lifecycle#> .
@prefix information:   <http://www.semanticweb.org/igonto/information#> .
@prefix enterprise:   <http://www.semanticweb.org/igonto/enterprise#> .


SELECT * WHERE{
  ?capability organization:partOfMaturityLevel organization:3_Essential.
  ?capability organization:capabilityLinkedToProcess ?process.
  ?process organization:processLinkedToOrganizationUnit ?organizationUnit. 
  ?process organization:riskDescription ?risk.
}ORDER BY ?capability""" .


###  http://www.semanticweb.org/igonto/igonto#Large_Q3
igonto:Large_Q3 rdf:type owl:NamedIndividual ,
                         igonto:Large_Enterprise_Query ;
                rdfs:comment """@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh:    <http://www.w3.org/ns/shacl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix igonto:   <http://www.semanticweb.org/igonto/igonto#> .
@prefix organization:   <http://www.semanticweb.org/igonto/organization#> .
@prefix jurisdiction:   <http://www.semanticweb.org/igonto/jurisdiction#> .
@prefix lifecycle:   <http://www.semanticweb.org/igonto/lifecycle#> .
@prefix information:   <http://www.semanticweb.org/igonto/information#> .
@prefix enterprise:   <http://www.semanticweb.org/igonto/enterprise#> .

# WHAT DOES THE Large ENTERPRISE NEED TO DO?
SELECT * WHERE {
  ?shape sh:group igonto:PositiveObjectPropertyGroup;
         sh:group igonto:InferenceObjectPropertyGroup.
  ?shape sh:path ?objectProperty.
  organization:Large_Enterprise_Instance ?objectProperty ?capability.
}""" .


###  http://www.semanticweb.org/igonto/igonto#Large_Q4
igonto:Large_Q4 rdf:type owl:NamedIndividual ,
                         igonto:Large_Enterprise_Query ;
                rdfs:comment """@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh:    <http://www.w3.org/ns/shacl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix igonto:   <http://www.semanticweb.org/igonto/igonto#> .
@prefix organization:   <http://www.semanticweb.org/igonto/organization#> .
@prefix jurisdiction:   <http://www.semanticweb.org/igonto/jurisdiction#> .
@prefix lifecycle:   <http://www.semanticweb.org/igonto/lifecycle#> .
@prefix information:   <http://www.semanticweb.org/igonto/information#> .
@prefix enterprise:   <http://www.semanticweb.org/igonto/enterprise#> .

# # WHAT DOES THE LARGE ENTERPRISE NOT NEED TO DO?
SELECT * WHERE {
  ?shape sh:group igonto:NegativeObjectPropertyGroup;
         sh:group igonto:InferenceObjectPropertyGroup.
  ?shape sh:path ?objectProperty.
  organization:Large_Enterprise_Instance ?objectProperty ?capability.
  OPTIONAL{?capability igonto:justifiedByArticle ?article}.
  OPTIONAL{?capability organization:linkedToProcess ?process}.
  OPTIONAL{?process organization:linkedToOrganizationUnit ?organizationUnit}.
}""" .


###  http://www.semanticweb.org/igonto/igonto#Large_Q5
igonto:Large_Q5 rdf:type owl:NamedIndividual ,
                         igonto:Large_Enterprise_Query ;
                rdfs:comment """@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh:    <http://www.w3.org/ns/shacl#> .
