###  http://www.semanticweb.org/igonto/information#Asset
information:Asset rdf:type owl:Class ;
                  rdfs:subClassOf information:Data ;
                  information:classDescription "Represents a data container that links digital content, metadata and their information together." .


###  http://www.semanticweb.org/igonto/information#Audit_Data
information:Audit_Data rdf:type owl:Class ;
                       rdfs:subClassOf information:Sensitive_Data ;
                       information:classDescription "Captures information related to audit trails and data activities and therfore critical and sensitive data." ;
                       rdfs:label "Audit Data" .


###  http://www.semanticweb.org/igonto/information#Audit_Trail
information:Audit_Trail rdf:type owl:Class ;
                        rdfs:subClassOf information:Record_Control_Structure ;
                        information:classDescription "\"Represents a systematic recording of record activities, including the capture of their access, modifications and other action information." ;
                        rdfs:label "Audit Trail" .


###  http://www.semanticweb.org/igonto/information#Category
information:Category rdf:type owl:Class ;
                     rdfs:subClassOf information:Record_Control_Structure ;
                     information:classDescription "Signifies the classification and categorization of Records." ;
                     rdfs:label "Category" .


###  http://www.semanticweb.org/igonto/information#Container
information:Container rdf:type owl:Class ;
                      rdfs:subClassOf information:Record ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty information:containsFile ;
                                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onClass information:File
                                      ] ;
                      information:classDescription "Represents a logical or physical entity to organize and store Data." ;
                      rdfs:label "Container" .


###  http://www.semanticweb.org/igonto/information#Content
information:Content rdf:type owl:Class ;
                    rdfs:subClassOf information:Data ;
                    information:classDescription "Represents valuable and concrete information of Data." .


###  http://www.semanticweb.org/igonto/information#Control_Data
information:Control_Data rdf:type owl:Class ;
                         rdfs:subClassOf information:Record_Control_Structure ;
                         rdfs:label "Control Data" .


###  http://www.semanticweb.org/igonto/information#Cost
information:Cost rdf:type owl:Class ;
                 rdfs:subClassOf [ rdf:type owl:Restriction ;
                                   owl:onProperty information:value ;
                                   owl:someValuesFrom xsd:string
                                 ] ;
                 information:classDescription "Represents the fincancial aspect of managing information assets. Cost is directly connected to the concept of Data." ;
                 rdfs:label "Cost" .


###  http://www.semanticweb.org/igonto/information#Data
information:Data rdf:type owl:Class ;
                 owl:equivalentClass information:Item ,
                                     organization:Data ,
                                     [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                              owl:onProperty information:hasComponent ;
                                                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                              owl:onClass information:Cost
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty information:hasComponent ;
                                                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                              owl:onClass information:Risk
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty information:hasComponent ;
                                                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                              owl:onClass information:Value
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                 rdfs:subClassOf [ rdf:type owl:Restriction ;
                                   owl:onProperty information:containsValue ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass information:Value
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty information:exposesRisk ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass information:Risk
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty information:producesCost ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass information:Cost
                                 ] ;
                 information:classDescription "Superclass of Data seen in the context of information value." ;
                 rdfs:label "Data" .


###  http://www.semanticweb.org/igonto/information#Data_Dictionary
information:Data_Dictionary rdf:type owl:Class ;
                            rdfs:subClassOf [ rdf:type owl:Restriction ;
                                              owl:onProperty information:vocabularyFor ;
                                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                              owl:onClass information:Taxonomy
                                            ] ;
                            information:classDescription "Represents the Data Dictionary of an enterprise that serves as vocabular for its Taxonomy." ;
                            rdfs:label "Data Dictionary" .


###  http://www.semanticweb.org/igonto/information#Document
information:Document rdf:type owl:Class ;
                     rdfs:subClassOf information:Data ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty information:hasEdition ;
                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onClass information:Edition
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty information:hasRendition ;
                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onClass information:Rendition
                                     ] ;
                     information:classDescription "Represents a structured representation of information that embodies specific versions and alternative presentations of a document with different formats, layouts, etc.." .


###  http://www.semanticweb.org/igonto/information#Edition
information:Edition rdf:type owl:Class ;
                    information:classDescription "Represents the versioning of information. Instances form this class capture changes, updates or revisions of Data." ;
                    rdfs:label "Edition" .


###  http://www.semanticweb.org/igonto/information#File
information:File rdf:type owl:Class ;
                 information:classDescription "Represents stored information in the form of digital or physical files." ;
                 rdfs:label "File" .


###  http://www.semanticweb.org/igonto/information#Folder
information:Folder rdf:type owl:Class ;
                   rdfs:subClassOf information:Container ;
                   information:classDescription "Represents a digital hierarchical organization unit to structure Documents and Data." ;
                   rdfs:label "Folder" .


###  http://www.semanticweb.org/igonto/information#Goal
information:Goal rdf:type owl:Class ;
                 owl:equivalentClass organization:Goal ;
                 rdfs:subClassOf [ rdf:type owl:Restriction ;
                                   owl:onProperty information:scope ;
                                   owl:someValuesFrom xsd:string
                                 ] ;
                 information:classDescription "Represents desired outcomes of the enterprise policy but put into context of information." ;
                 rdfs:label "Goal" .


###  http://www.semanticweb.org/igonto/information#Hold
information:Hold rdf:type owl:Class ;
                 rdfs:subClassOf information:Record_Control_Structure ;
                 information:classDescription "Represents the hold process for the temporary suspension of record management processes and activities." ;
                 rdfs:label "Hold" .


###  http://www.semanticweb.org/igonto/information#Information
information:Information rdf:type owl:Class ;
                        rdfs:subClassOf information:Metadata ;
                        information:classDescription "Represents different types of information that can be derived form Data (Content information) or possibilites to describe the Data (Archival information, description)." .


###  http://www.semanticweb.org/igonto/information#Item
information:Item rdf:type owl:Class ;
                 rdfs:subClassOf [ rdf:type owl:Restriction ;
                                   owl:onProperty information:hasVersion ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass information:Version
                                 ] ;
                 information:classDescription "Annother representation of the conept of Data but seen as an item with versioning." .


###  http://www.semanticweb.org/igonto/information#Metadata
information:Metadata rdf:type owl:Class ;
                     rdfs:subClassOf information:Data ;
                     information:classDescription "Describes the concept of metadata driven by its subclass information." .


###  http://www.semanticweb.org/igonto/information#Metric
information:Metric rdf:type owl:Class ;
                   rdfs:subClassOf [ rdf:type owl:Restriction ;
                                     owl:onProperty information:metricUnit ;
                                     owl:someValuesFrom xsd:string
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty information:value ;
                                     owl:someValuesFrom xsd:string
                                   ] ;
                   information:classDescription "Represents quantifiable measures used to evaluate and to capture apsects of data quality, security and compliance." ;
                   rdfs:label "Metric" .


###  http://www.semanticweb.org/igonto/information#PII_Data
information:PII_Data rdf:type owl:Class ;
                     rdfs:subClassOf information:Sensitive_Data ;
                     information:classDescription "Represents Personally Identifiable Informatoion that directly or indirectly identify individuals." ;
                     rdfs:label "PII Data" .


###  http://www.semanticweb.org/igonto/information#Personnel_Files
information:Personnel_Files rdf:type owl:Class ;
                            rdfs:subClassOf information:Sensitive_Data ;
                            information:classDescription "Represents confidential information about individuals including employee related data." ;
                            rdfs:label "Personnel Files" .


###  http://www.semanticweb.org/igonto/information#Policy
information:Policy rdf:type owl:Class ;
                   owl:equivalentClass organization:IG_Policy ,
                                       [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                owl:onProperty information:hasComponent ;
                                                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                owl:onClass information:Goal
                                                              ]
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty information:hasComponent ;
                                                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                owl:onClass information:Metric
                                                              ]
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty information:hasComponent ;
                                                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                owl:onClass information:Rule
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ;
                   rdfs:subClassOf [ rdf:type owl:Restriction ;
                                     owl:onProperty information:appliesRule ;
                                     owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass information:Rule
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty information:employsRule ;
                                     owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass information:Rule
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty information:grantsPriviligeToRole ;
                                     owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass information:Role
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty information:hasGoal ;
                                     owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass information:Goal
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty information:monitorsMetric ;
                                     owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass information:Metric
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty information:scope ;
                                     owl:someValuesFrom xsd:string
                                   ] ;
                   information:classDescription "Reprsents authoritative guidelines for information withing an enteprise regarding its management, use and protection. It grants priviliges to Roles, applies Rules and try to acheve a specific Goal." ;
                   rdfs:label "Policy" .


###  http://www.semanticweb.org/igonto/information#Privilige
information:Privilige rdf:type owl:Class ;
                      information:classDescription "Represents information access rights given to roles. Each Privilige is part of a Policy." ;
                      rdfs:label "Privilige" .


###  http://www.semanticweb.org/igonto/information#Record
information:Record rdf:type owl:Class ;
                   owl:equivalentClass organization:Record ;
                   rdfs:subClassOf [ rdf:type owl:Restriction ;
                                     owl:onProperty information:classifiesData ;
                                     owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass information:Data
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty information:hasRecordControlStructure ;
                                     owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass information:Record_Control_Structure
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty information:usesDataDictionary ;
                                     owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass information:Data_Dictionary
                                   ] ;
                   information:classDescription "Represents processing records seen from an information persepctive. Every record has a Record Control Structure, uses a specific vocabulary from the Data Dictionary and classifies Data." ;
                   rdfs:label "Record" .


###  http://www.semanticweb.org/igonto/information#Record_Control_Structure
information:Record_Control_Structure rdf:type owl:Class ;
                                     owl:equivalentClass organization:Record_Control_Structure ;
                                     rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                       owl:onProperty information:endDate ;
                                                       owl:someValuesFrom xsd:dateTime
                                                     ] ,
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty information:startDate ;
                                                       owl:someValuesFrom xsd:dateTime
                                                     ] ;
                                     information:classDescription "Represents processes and mechanisms for creating, maintaining and disposing of Records." ;
                                     rdfs:label "Record Control Structure" .


###  http://www.semanticweb.org/igonto/information#Rendition
information:Rendition rdf:type owl:Class ;
                      information:classDescription "Represents alternative presentations of information assets." ;
                      rdfs:label "Rendition" .


###  http://www.semanticweb.org/igonto/information#Report
information:Report rdf:type owl:Class ;
                   rdfs:subClassOf information:Record_Control_Structure ;
                   information:classDescription "Represents the reporting process of records for communication and decision-making purposes." ;
                   rdfs:label "Report" .


###  http://www.semanticweb.org/igonto/information#Retention
information:Retention rdf:type owl:Class ;
                      rdfs:subClassOf information:Record_Control_Structure ;
                      information:classDescription "Represents the retention process of records which decide how long records are detained, when they are transferred to an archival storage or whenn they are disposed of." ;
                      rdfs:label "Retention" .


###  http://www.semanticweb.org/igonto/information#Risk
information:Risk rdf:type owl:Class ;
                 information:classDescription "Represents the concept of risk related to data in the context of security, compliance or reputational risks." ;
                 rdfs:label "Risk" .


###  http://www.semanticweb.org/igonto/information#Role
information:Role rdf:type owl:Class ;
                 owl:equivalentClass organization:Role ;
                 rdfs:subClassOf [ rdf:type owl:Restriction ;
                                   owl:onProperty information:hasPrivilige ;
                                   owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass information:Privilige
                                 ] ;
                 information:classDescription "Represents responisbilites and permissions assigned to individuals to access, manage or oversee information." ;
                 rdfs:label "Role" .


###  http://www.semanticweb.org/igonto/information#Rule
information:Rule rdf:type owl:Class ;
                 owl:equivalentClass organization:Rule ;
                 rdfs:subClassOf [ rdf:type owl:Restriction ;
                                   owl:onProperty information:definesSchedule ;
                                   owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass information:Schedule
