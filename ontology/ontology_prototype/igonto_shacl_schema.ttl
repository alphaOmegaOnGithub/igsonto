@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh:    <http://www.w3.org/ns/shacl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex:    <http://www.example.org/#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix igonto:   <http://www.semanticweb.org/igonto/igonto#> .
@prefix organization: <http://www.semanticweb.org/igonto/organization#> .
@prefix igsonto-complete: <http://www.semanticweb.org/igsonto/igsonto-complete#> .

igonto:InferenceObjectPropertyGroup a sh:PropertyGroup ;
   rdfs:label "Object properties that are generated by Inference" .

igonto:NegativeObjectPropertyGroup a sh:PropertyGroup ;
   rdfs:label "Object properties that explicitly express a negative relationship" .

igonto:PositiveObjectPropertyGroup a sh:PropertyGroup ;
   rdfs:label "Normal Object Properties" .

igonto:enterpriseDoesNOTNeedToFullfillCapabilityShape a sh:PropertyShape ;
    sh:group igonto:NegativeObjectPropertyGroup ;
    sh:group igonto:InferenceObjectPropertyGroup ;
    sh:path igonto:enterpriseDoesNOTNeedToFullfillCapability .

igonto:enterpriseNeedsToFullfillCapabilityShape a sh:PropertyShape ;
    sh:group igonto:PositiveObjectPropertyGroup ;
    sh:group igonto:InferenceObjectPropertyGroup ;
    sh:path igonto:enterpriseNeedsToFullfillCapability .

igonto:needsArchitectureShape a sh:PropertyShape ;
    sh:group igonto:PositiveObjectPropertyGroup ;
    sh:group igonto:InferenceObjectPropertyGroup ;
    sh:path organization:needsArchitecture .

igonto:needsDesignShape a sh:PropertyShape ;
    sh:group igonto:PositiveObjectPropertyGroup ;
    sh:group igonto:InferenceObjectPropertyGroup ;
    sh:path organization:needsDesign .

igonto:needsBlueprintShape a sh:PropertyShape ;
    sh:group igonto:PositiveObjectPropertyGroup ;
    sh:group igonto:InferenceObjectPropertyGroup ;
    sh:path organization:needsBlueprint .

igsonto-complete:designDoesNOTNeedComponentShape a sh:PropertyShape ;
    sh:group igonto:NegativeObjectPropertyGroup ;
    sh:group igonto:InferenceObjectPropertyGroup ;
    sh:path igsonto-complete:designDoesNOTNeedComponent .


igsonto-complete:architectureDoesNOTNeedServiceShape a sh:PropertyShape ;
    sh:group igonto:NegativeObjectPropertyGroup ;
    sh:group igonto:InferenceObjectPropertyGroup ;
    sh:path igsonto-complete:architectureDoesNOTNeedService .