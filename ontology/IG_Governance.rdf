        <Governance:needsTechnicalRequirement rdf:resource="http://www.semanticweb.org/IG/Governance#Document_Management_System_1"/>
        <Governance:needsTechnicalRequirement rdf:resource="http://www.semanticweb.org/IG/Governance#Metadata_Framework_1"/>
        <Governance:needsTechnicalRequirement rdf:resource="http://www.semanticweb.org/IG/Governance#Records_Retention_Schedule_1"/>
