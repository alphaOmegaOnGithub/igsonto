                            <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/ontology/FND/Law/LegalCore/Constitution"/>
                            <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/ontology/FND/Law/LegalCore/Law"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
