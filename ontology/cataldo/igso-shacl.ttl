@prefix : <http://www.w3.org/ns/shacl#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.w3.org/ns/shacl#> .

<http://www.w3.org/ns/shacl#> rdf:type owl:Ontology ;
                               rdfs:comment "This vocabulary defines terms used in SHACL, the W3C Shapes Constraint Language."@en ;
                               rdfs:label "W3C Shapes Constraint Language (SHACL) Vocabulary"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/ns/shacl#annotationValue
sh:annotationValue rdfs:comment "The (default) values of the annotation property."@en ;
                   rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
                   rdfs:label "annotation value"@en ;
                   rdf:type owl:AnnotationProperty ;
                   rdfs:domain sh:ResultAnnotation .


###  http://www.w3.org/ns/shacl#defaultValue
sh:defaultValue rdfs:comment "A default value for a property, for example for user interface tools to pre-populate input fields."@en ;
                rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
                rdfs:label "default value"@en ;
                rdf:type owl:AnnotationProperty ;
                rdfs:domain sh:PropertyShape .


###  http://www.w3.org/ns/shacl#description
sh:description rdfs:comment "Human-readable descriptions for the property in the context of the surrounding shape."@en ;
               rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
               rdfs:label "description"@en ;
               rdf:type owl:AnnotationProperty ;
               rdfs:domain sh:PropertyShape .


###  http://www.w3.org/ns/shacl#focusNode
sh:focusNode rdfs:comment "The focus node that was validated when the result was produced."@en ;
             rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
             rdfs:label "focus node"@en ;
             rdf:type owl:AnnotationProperty ;
             rdfs:domain sh:AbstractResult .


###  http://www.w3.org/ns/shacl#labelTemplate
sh:labelTemplate rdfs:comment "Outlines how human-readable labels of instances of the associated Parameterizable shall be produced. The values can contain {?paramName} as placeholders for the actual values of the given parameter."@en ;
                 rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
                 rdfs:label "label template"@en ;
                 rdf:type owl:AnnotationProperty ;
                 rdfs:domain sh:Parameterizable .


###  http://www.w3.org/ns/shacl#name
sh:name rdfs:comment "Human-readable labels for the property in the context of the surrounding shape."@en ;
        rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
        rdfs:label "name"@en ;
        rdf:type owl:AnnotationProperty ;
        rdfs:domain sh:PropertyShape .


###  http://www.w3.org/ns/shacl#object
sh:object rdfs:comment "An expression producing the nodes that shall be inferred as objects."@en ;
          rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
          rdfs:label "object"@en ;
          rdf:type owl:AnnotationProperty ;
          rdfs:domain sh:TripleRule .


###  http://www.w3.org/ns/shacl#predicate
sh:predicate rdfs:comment "An expression producing the properties that shall be inferred as predicates."@en ;
             rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
             rdfs:label "predicate"@en ;
             rdf:type owl:AnnotationProperty ;
             rdfs:domain sh:TripleRule .


###  http://www.w3.org/ns/shacl#resultMessage
sh:resultMessage rdfs:comment "Human-readable messages explaining the cause of the result."@en ;
                 rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
                 rdfs:label "result message"@en ;
                 rdf:type owl:AnnotationProperty ;
                 rdfs:domain sh:AbstractResult .


###  http://www.w3.org/ns/shacl#sourceConstraint
sh:sourceConstraint rdfs:comment "The constraint that was validated when the result was produced."@en ;
                    rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
                    rdfs:label "source constraint"@en ;
                    rdf:type owl:AnnotationProperty ;
                    rdfs:domain sh:AbstractResult .


###  http://www.w3.org/ns/shacl#subject
sh:subject rdfs:comment "An expression producing the resources that shall be inferred as subjects."@en ;
           rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
           rdfs:label "subject"@en ;
           rdf:type owl:AnnotationProperty ;
           rdfs:domain sh:TripleRule .


###  http://www.w3.org/ns/shacl#targetNode
sh:targetNode rdfs:comment "Links a shape to individual nodes, indicating that these nodes must conform to the shape."@en ;
              rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
              rdfs:label "target node"@en ;
              rdf:type owl:AnnotationProperty ;
              rdfs:domain sh:Shape .


###  http://www.w3.org/ns/shacl#value
sh:value rdfs:comment "An RDF node that has caused the result."@en ;
         rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
         rdfs:label "value"@en ;
         rdf:type owl:AnnotationProperty ;
         rdfs:domain sh:AbstractResult .


#################################################################
#    Object Properties
#################################################################

###  http://www.w3.org/ns/shacl#datatype
sh:datatype rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/shacl#declare
sh:declare rdf:type owl:ObjectProperty ;
           rdfs:comment "Links a resource with its namespace prefix declarations."@en ;
           rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
           rdfs:label "declare"@en .


###  http://www.w3.org/ns/shacl#in
sh:in rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/shacl#nodeKind
sh:nodeKind rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/shacl#parameter
sh:parameter rdf:type owl:ObjectProperty ;
             rdfs:comment "The parameters of a function or constraint component."@en ;
             rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
             rdfs:label "parameter"@en .


###  http://www.w3.org/ns/shacl#path
sh:path rdf:type owl:ObjectProperty ;
        rdfs:comment "Specifies the property path of a property shape."@en ;
        rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
        rdfs:label "path"@en .


###  http://www.w3.org/ns/shacl#suggestedShapesGraph
sh:suggestedShapesGraph rdf:type owl:ObjectProperty ;
                        rdfs:comment "Suggested shapes graphs for this ontology. The values of this property may be used in the absence of specific sh:shapesGraph statements."@en ;
                        rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
                        rdfs:label "suggested shapes graph"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.w3.org/ns/shacl#annotationVarName
sh:annotationVarName rdf:type owl:DatatypeProperty ;
                     rdfs:range xsd:string ;
                     rdfs:comment "The name of the SPARQL variable from the SELECT clause that shall be used for the values."@en ;
                     rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
                     rdfs:label "annotation variable name"@en .


###  http://www.w3.org/ns/shacl#ask
sh:ask rdf:type owl:DatatypeProperty ;
       rdfs:range xsd:string ;
       rdfs:comment "The SPARQL ASK query to execute."@en ;
       rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
       rdfs:label "ask"@en .


###  http://www.w3.org/ns/shacl#closed
sh:closed rdf:type owl:DatatypeProperty ;
          rdfs:range xsd:boolean .


###  http://www.w3.org/ns/shacl#conforms
sh:conforms rdf:type owl:DatatypeProperty ;
            rdfs:range xsd:boolean ;
            rdfs:comment "True if the validation did not produce any validation results, and false otherwise."@en ;
            rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
            rdfs:label "conforms"@en .


###  http://www.w3.org/ns/shacl#construct
sh:construct rdf:type owl:DatatypeProperty ;
             rdfs:range xsd:string ;
             rdfs:comment "The SPARQL CONSTRUCT query to execute."@en ;
             rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
             rdfs:label "construct"@en .


###  http://www.w3.org/ns/shacl#deactivated
sh:deactivated rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:boolean ;
               rdfs:comment "If set to true then all nodes conform to this."@en ;
               rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
               rdfs:label "deactivated"@en .


###  http://www.w3.org/ns/shacl#flags
sh:flags rdf:type owl:DatatypeProperty ;
         rdfs:range xsd:string .


###  http://www.w3.org/ns/shacl#jsFunctionName
sh:jsFunctionName rdf:type owl:DatatypeProperty ;
                  rdfs:range xsd:string ;
                  rdfs:comment "The name of the JavaScript function to execute."@en ;
                  rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
                  rdfs:label "JavaScript function name"@en .


###  http://www.w3.org/ns/shacl#jsLibraryURL
sh:jsLibraryURL rdf:type owl:DatatypeProperty ;
                rdfs:range xsd:anyURI ;
                rdfs:comment "Declares the URLs of a JavaScript library. This should be the absolute URL of a JavaScript file. Implementations may redirect those to local files."@en ;
                rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
                rdfs:label "JavaScript library URL"@en .


###  http://www.w3.org/ns/shacl#maxCount
sh:maxCount rdf:type owl:DatatypeProperty ;
            rdfs:range xsd:integer .


###  http://www.w3.org/ns/shacl#maxLength
sh:maxLength rdf:type owl:DatatypeProperty ;
             rdfs:range xsd:integer .


###  http://www.w3.org/ns/shacl#minCount
sh:minCount rdf:type owl:DatatypeProperty ;
            rdfs:range xsd:integer .


###  http://www.w3.org/ns/shacl#minLength
sh:minLength rdf:type owl:DatatypeProperty ;
             rdfs:range xsd:integer .


###  http://www.w3.org/ns/shacl#namespace
sh:namespace rdf:type owl:DatatypeProperty ;
             rdfs:range xsd:anyURI ;
             rdfs:comment "The namespace associated with a prefix in a prefix declaration."@en ;
             rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
             rdfs:label "namespace"@en .


###  http://www.w3.org/ns/shacl#optional
sh:optional rdf:type owl:DatatypeProperty ;
            rdfs:range xsd:boolean ;
            rdfs:comment "Indicates whether a parameter is optional."@en ;
            rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
            rdfs:label "optional"@en .


###  http://www.w3.org/ns/shacl#pattern
sh:pattern rdf:type owl:DatatypeProperty ;
           rdfs:range xsd:string .


###  http://www.w3.org/ns/shacl#prefix
sh:prefix rdf:type owl:DatatypeProperty ;
          rdfs:range xsd:string ;
          rdfs:comment "The prefix of a prefix declaration."@en ;
          rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
          rdfs:label "prefix"@en .


###  http://www.w3.org/ns/shacl#qualifiedMaxCount
sh:qualifiedMaxCount rdf:type owl:DatatypeProperty ;
                     rdfs:range xsd:integer .


###  http://www.w3.org/ns/shacl#qualifiedMinCount
sh:qualifiedMinCount rdf:type owl:DatatypeProperty ;
                     rdfs:range xsd:integer .


###  http://www.w3.org/ns/shacl#qualifiedValueShapesDisjoint
sh:qualifiedValueShapesDisjoint rdf:type owl:DatatypeProperty ;
                                rdfs:range xsd:boolean .


###  http://www.w3.org/ns/shacl#select
sh:select rdf:type owl:DatatypeProperty ;
          rdfs:range xsd:string ;
          rdfs:comment "The SPARQL SELECT query to execute."@en ;
          rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
          rdfs:label "select"@en .


###  http://www.w3.org/ns/shacl#shapesGraphWellFormed
sh:shapesGraphWellFormed rdf:type owl:DatatypeProperty ;
                         rdfs:range xsd:boolean ;
                         rdfs:comment "If true then the validation engine was certain that the shapes graph has passed all SHACL syntax requirements during the validation process."@en ;
                         rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
                         rdfs:label "shapes graph well-formed"@en .


###  http://www.w3.org/ns/shacl#uniqueLang
sh:uniqueLang rdf:type owl:DatatypeProperty ;
              rdfs:range xsd:boolean .


###  http://www.w3.org/ns/shacl#update
sh:update rdf:type owl:DatatypeProperty ;
          rdfs:range xsd:string ;
          rdfs:comment "The SPARQL UPDATE to execute."@en ;
          rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
          rdfs:label "update"@en .


#################################################################
#    Individuals
#################################################################

###  http://www.w3.org/2001/XMLSchema#boolean
xsd:boolean rdf:type owl:NamedIndividual .


###  http://www.w3.org/2001/XMLSchema#integer
xsd:integer rdf:type owl:NamedIndividual .


###  http://www.w3.org/2001/XMLSchema#string
xsd:string rdf:type owl:NamedIndividual .


###  http://www.w3.org/ns/shacl#
<http://www.w3.org/ns/shacl#> rdf:type owl:NamedIndividual ;
                              sh:declare [ sh:namespace "http://www.w3.org/ns/shacl#" ;
                                           sh:prefix "sh"
                                         ] ;
                              sh:suggestedShapesGraph <http://www.w3.org/ns/shacl-shacl#> .


###  http://www.w3.org/ns/shacl#AndConstraintComponent
sh:AndConstraintComponent rdf:type owl:NamedIndividual ;
                          sh:parameter sh:AndConstraintComponent-and ;
                          rdfs:comment "A constraint component that can be used to test whether a value node conforms to all members of a provided list of shapes."@en ;
                          rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
                          rdfs:label "And constraint component"@en .


###  http://www.w3.org/ns/shacl#AndConstraintComponent-and
sh:AndConstraintComponent-and rdf:type owl:NamedIndividual ;
                              sh:path sh:and ;
                              rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> .


###  http://www.w3.org/ns/shacl#ClassConstraintComponent
sh:ClassConstraintComponent rdf:type owl:NamedIndividual ;
                            sh:parameter sh:ClassConstraintComponent-class ;
                            rdfs:comment "A constraint component that can be used to verify that each value node is an instance of a given type."@en ;
                            rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
                            rdfs:label "Class constraint component"@en .


###  http://www.w3.org/ns/shacl#ClassConstraintComponent-class
sh:ClassConstraintComponent-class rdf:type owl:NamedIndividual ;
                                  sh:nodeKind sh:IRI ;
                                  sh:path sh:class ;
                                  rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> .


###  http://www.w3.org/ns/shacl#ClosedConstraintComponent
sh:ClosedConstraintComponent rdf:type owl:NamedIndividual ;
                             sh:parameter sh:ClosedConstraintComponent-closed ,
                                          sh:ClosedConstraintComponent-ignoredProperties ;
                             rdfs:comment "A constraint component that can be used to indicate that focus nodes must only have values for those properties that have been explicitly enumerated via sh:property/sh:path."@en ;
                             rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
                             rdfs:label "Closed constraint component"@en .


###  http://www.w3.org/ns/shacl#ClosedConstraintComponent-closed
sh:ClosedConstraintComponent-closed rdf:type owl:NamedIndividual ;
                                    sh:datatype xsd:boolean ;
                                    sh:path sh:closed ;
                                    rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> .


###  http://www.w3.org/ns/shacl#ClosedConstraintComponent-ignoredProperties
sh:ClosedConstraintComponent-ignoredProperties rdf:type owl:NamedIndividual ;
                                               sh:path sh:ignoredProperties ;
                                               sh:optional "true"^^xsd:boolean ;
                                               rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> .


###  http://www.w3.org/ns/shacl#DatatypeConstraintComponent
sh:DatatypeConstraintComponent rdf:type owl:NamedIndividual ;
                               sh:parameter sh:DatatypeConstraintComponent-datatype ;
                               rdfs:comment "A constraint component that can be used to restrict the datatype of all value nodes."@en ;
                               rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
                               rdfs:label "Datatype constraint component"@en .


###  http://www.w3.org/ns/shacl#DatatypeConstraintComponent-datatype
sh:DatatypeConstraintComponent-datatype rdf:type owl:NamedIndividual ;
                                        sh:nodeKind sh:IRI ;
                                        sh:path sh:datatype ;
                                        sh:maxCount 1 ;
                                        rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> .


###  http://www.w3.org/ns/shacl#DisjointConstraintComponent
sh:DisjointConstraintComponent rdf:type owl:NamedIndividual ;
                               sh:parameter sh:DisjointConstraintComponent-disjoint ;
                               rdfs:comment "A constraint component that can be used to verify that the set of value nodes is disjoint with the the set of nodes that have the focus node as subject and the value of a given property as predicate."@en ;
                               rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
                               rdfs:label "Disjoint constraint component"@en .


###  http://www.w3.org/ns/shacl#DisjointConstraintComponent-disjoint
sh:DisjointConstraintComponent-disjoint rdf:type owl:NamedIndividual ;
                                        sh:nodeKind sh:IRI ;
                                        sh:path sh:disjoint ;
                                        rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> .


###  http://www.w3.org/ns/shacl#EqualsConstraintComponent
sh:EqualsConstraintComponent rdf:type owl:NamedIndividual ;
                             sh:parameter sh:EqualsConstraintComponent-equals ;
                             rdfs:comment "A constraint component that can be used to verify that the set of value nodes is equal to the set of nodes that have the focus node as subject and the value of a given property as predicate."@en ;
                             rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
                             rdfs:label "Equals constraint component"@en .


###  http://www.w3.org/ns/shacl#EqualsConstraintComponent-equals
sh:EqualsConstraintComponent-equals rdf:type owl:NamedIndividual ;
                                    sh:nodeKind sh:IRI ;
                                    sh:path sh:equals ;
                                    rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> .


###  http://www.w3.org/ns/shacl#ExpressionConstraintComponent
sh:ExpressionConstraintComponent rdf:type owl:NamedIndividual ;
                                 sh:parameter sh:ExpressionConstraintComponent-expression ;
                                 rdfs:comment "A constraint component that can be used to verify that a given node expression produces true for all value nodes."@en ;
                                 rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
                                 rdfs:label "Expression constraint component"@en .


###  http://www.w3.org/ns/shacl#ExpressionConstraintComponent-expression
sh:ExpressionConstraintComponent-expression rdf:type owl:NamedIndividual ;
                                            sh:path sh:expression ;
                                            rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> .


###  http://www.w3.org/ns/shacl#HasValueConstraintComponent
sh:HasValueConstraintComponent rdf:type owl:NamedIndividual ;
                               sh:parameter sh:HasValueConstraintComponent-hasValue ;
                               rdfs:comment "A constraint component that can be used to verify that one of the value nodes is a given RDF node."@en ;
                               rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
                               rdfs:label "Has-value constraint component"@en .


###  http://www.w3.org/ns/shacl#HasValueConstraintComponent-hasValue
sh:HasValueConstraintComponent-hasValue rdf:type owl:NamedIndividual ;
                                        sh:path sh:hasValue ;
                                        rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> .


###  http://www.w3.org/ns/shacl#IRI
sh:IRI rdf:type owl:NamedIndividual ;
       rdfs:comment "The node kind of all IRIs."@en ;
       rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
       rdfs:label "IRI"@en .


###  http://www.w3.org/ns/shacl#InConstraintComponent
sh:InConstraintComponent rdf:type owl:NamedIndividual ;
                         sh:parameter sh:InConstraintComponent-in ;
                         rdfs:comment "A constraint component that can be used to exclusively enumerate the permitted value nodes."@en ;
                         rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
                         rdfs:label "In constraint component"@en .


###  http://www.w3.org/ns/shacl#InConstraintComponent-in
sh:InConstraintComponent-in rdf:type owl:NamedIndividual ;
                            sh:path sh:in ;
                            sh:maxCount 1 ;
                            rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> .


###  http://www.w3.org/ns/shacl#JSConstraint-js
sh:JSConstraint-js rdf:type owl:NamedIndividual ;
                   sh:path sh:js ;
                   rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> .


###  http://www.w3.org/ns/shacl#JSConstraintComponent
sh:JSConstraintComponent rdf:type owl:NamedIndividual ;
                         sh:parameter sh:JSConstraint-js ;
                         rdfs:comment "A constraint component with the parameter sh:js linking to a sh:JSConstraint containing a sh:script."@en ;
                         rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
                         rdfs:label "JavaScript constraint component"@en .


###  http://www.w3.org/ns/shacl#LanguageInConstraintComponent
sh:LanguageInConstraintComponent rdf:type owl:NamedIndividual ;
                                 sh:parameter sh:LanguageInConstraintComponent-languageIn ;
                                 rdfs:comment "A constraint component that can be used to enumerate language tags that all value nodes must have."@en ;
                                 rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
                                 rdfs:label "Language-in constraint component"@en .


###  http://www.w3.org/ns/shacl#LanguageInConstraintComponent-languageIn
sh:LanguageInConstraintComponent-languageIn rdf:type owl:NamedIndividual ;
                                            sh:path sh:languageIn ;
                                            sh:maxCount 1 ;
                                            rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> .


###  http://www.w3.org/ns/shacl#LessThanConstraintComponent
sh:LessThanConstraintComponent rdf:type owl:NamedIndividual ;
                               sh:parameter sh:LessThanConstraintComponent-lessThan ;
                               rdfs:comment "A constraint component that can be used to verify that each value node is smaller than all the nodes that have the focus node as subject and the value of a given property as predicate."@en ;
                               rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
                               rdfs:label "Less-than constraint component"@en .


###  http://www.w3.org/ns/shacl#LessThanConstraintComponent-lessThan
sh:LessThanConstraintComponent-lessThan rdf:type owl:NamedIndividual ;
                                        sh:nodeKind sh:IRI ;
                                        sh:path sh:lessThan ;
                                        rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> .


###  http://www.w3.org/ns/shacl#LessThanOrEqualsConstraintComponent
sh:LessThanOrEqualsConstraintComponent rdf:type owl:NamedIndividual ;
                                       sh:parameter sh:LessThanOrEqualsConstraintComponent-lessThanOrEquals ;
                                       rdfs:comment "A constraint component that can be used to verify that every value node is smaller than all the nodes that have the focus node as subject and the value of a given property as predicate."@en ;
                                       rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
                                       rdfs:label "less-than-or-equals constraint component"@en .


###  http://www.w3.org/ns/shacl#LessThanOrEqualsConstraintComponent-lessThanOrEquals
sh:LessThanOrEqualsConstraintComponent-lessThanOrEquals rdf:type owl:NamedIndividual ;
                                                        sh:nodeKind sh:IRI ;
                                                        sh:path sh:lessThanOrEquals ;
                                                        rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> .


###  http://www.w3.org/ns/shacl#Literal
sh:Literal rdf:type owl:NamedIndividual ;
           rdfs:comment "The node kind of all literals."@en ;
           rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
           rdfs:label "Literal"@en .


###  http://www.w3.org/ns/shacl#MaxCountConstraintComponent
sh:MaxCountConstraintComponent rdf:type owl:NamedIndividual ;
                               sh:parameter sh:MaxCountConstraintComponent-maxCount ;
                               rdfs:comment "A constraint component that can be used to restrict the maximum number of value nodes."@en ;
                               rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> ;
                               rdfs:label "Max-count constraint component"@en .


###  http://www.w3.org/ns/shacl#MaxCountConstraintComponent-maxCount
sh:MaxCountConstraintComponent-maxCount rdf:type owl:NamedIndividual ;
                                        sh:datatype xsd:integer ;
                                        sh:path sh:maxCount ;
                                        sh:maxCount 1 ;
                                        rdfs:isDefinedBy <http://www.w3.org/ns/shacl#> .


###  http://www.w3.org/ns/shacl#MaxExclusiveConstraintComponent
