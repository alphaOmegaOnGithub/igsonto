                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="https://www.omg.org/spec/Commons/DatesAndTimes/CombinedDateTime"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:isDefinedBy rdf:resource="https://spec.edmcouncil.org/fibo/ontology/IND/Indicators/Indicators/"/>
        <rdfs:label>end-of-day market rate</rdfs:label>
        <skos:definition>value of a given market rate of the end of the business day for a specific date</skos:definition>
    </owl:Class>
    


