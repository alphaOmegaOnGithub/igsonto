        <rdfs:subPropertyOf rdf:resource="https://www.omg.org/spec/Commons/ContextualDesignators/appliesTo"/>
        <rdfs:isDefinedBy rdf:resource="https://spec.edmcouncil.org/fibo/ontology/IND/Indicators/Indicators/"/>
        <rdfs:label>is volatility of</rdfs:label>
        <skos:definition>indicates something to which the volatility measure applies and of which it is a measure</skos:definition>
    </owl:ObjectProperty>
    


