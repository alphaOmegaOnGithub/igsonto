              rdfs:subClassOf :Rule .


###  http://www.example.org/cm/igso/2023/igso-data#TransferTrigger
:TransferTrigger rdf:type owl:Class ;
                 rdfs:subClassOf :TriggerType ;
                 rdfs:label "TransferTrigger" .


###  http://www.example.org/cm/igso/2023/igso-data#TransferType
:TransferType rdf:type owl:Class ;
              rdfs:subClassOf :DataType ;
              rdfs:label "TransferType" .


