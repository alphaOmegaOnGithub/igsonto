###  http://www.example.org/cm/igso/2023/igso-data#StorageLocation
:StorageLocation rdf:type owl:Class ;
                 rdfs:subClassOf :StorageType .


###  http://www.example.org/cm/igso/2023/igso-data#StorageMigrationTrigger
:StorageMigrationTrigger rdf:type owl:Class ;
                         rdfs:subClassOf :TriggerType ;
                         rdfs:label "StorageMigrationTrigger" .


###  http://www.example.org/cm/igso/2023/igso-data#StoragePolicy
:StoragePolicy rdf:type owl:Class ;
               rdfs:subClassOf :Policy .


###  http://www.example.org/cm/igso/2023/igso-data#StorageRule
:StorageRule rdf:type owl:Class ;
             rdfs:subClassOf :Rule .


