               rdfs:label "LifecycleType" .


###  http://www.example.org/cm/igso/2023/igso-data#MetadataType
:MetadataType rdf:type owl:Class ;
              rdfs:subClassOf :DataType ;
              rdfs:comment "MetadataType" .


###  http://www.example.org/cm/igso/2023/igso-data#MultimediaObject
:MultimediaObject rdf:type owl:Class ;
                  rdfs:subClassOf :ContentObject .


###  http://www.example.org/cm/igso/2023/igso-data#NewSQLDatabase
:NewSQLDatabase rdf:type owl:Class ;
                rdfs:subClassOf :Database .


###  http://www.example.org/cm/igso/2023/igso-data#NoSQLDatabase
:NoSQLDatabase rdf:type owl:Class ;
               rdfs:subClassOf :Database .


###  http://www.example.org/cm/igso/2023/igso-data#Notice
:Notice rdf:type owl:Class ;
        rdfs:subClassOf :Document .


