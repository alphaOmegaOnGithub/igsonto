dcat:catalog rdf:type owl:ObjectProperty ;
             rdfs:domain dcat:Catalog ;
             rdfs:range dcat:Catalog .


###  http://www.w3.org/ns/dcat#record
dcat:record rdf:type owl:ObjectProperty ;
            rdfs:domain dcat:Catalog .


#################################################################
#    Classes
#################################################################

###  http://purl.org/dc/dcmitype/Collection
dctype:Collection rdf:type owl:Class .


###  http://purl.org/dc/dcmitype/Dataset
dctype:Dataset rdf:type owl:Class .


###  http://purl.org/dc/terms/Policy
dct:Policy rdf:type owl:Class ;
           owl:equivalentClass :Policy .


###  http://www.example.org/cm/igso/2023/igso-data#AccessControlList
:AccessControlList rdf:type owl:Class ;
                   rdfs:subClassOf :MetadataType ;
                   rdfs:label "AccessControlList" ;
                   cmns-av:synonym "ACL" .


###  http://www.example.org/cm/igso/2023/igso-data#AssetType
:AssetType rdf:type owl:Class ;
           rdfs:subClassOf :DataType ;
           rdfs:label "AssetType" .


###  http://www.example.org/cm/igso/2023/igso-data#BlockStorage
:BlockStorage rdf:type owl:Class ;
              rdfs:subClassOf :StorageType .


###  http://www.example.org/cm/igso/2023/igso-data#BusinessAsset
:BusinessAsset rdf:type owl:Class ;
               rdfs:subClassOf :AssetType .


###  http://www.example.org/cm/igso/2023/igso-data#BusinessData
:BusinessData rdf:type owl:Class ;
              rdfs:subClassOf :BusinessAsset .


###  http://www.example.org/cm/igso/2023/igso-data#BusinessPartnerData
:BusinessPartnerData rdf:type owl:Class ;
                     rdfs:subClassOf :BusinessAsset .


###  http://www.example.org/cm/igso/2023/igso-data#ContentAsset
:ContentAsset rdf:type owl:Class ;
              rdfs:subClassOf :AssetType ;
              owl:disjointWith :DataAsset .


###  http://www.example.org/cm/igso/2023/igso-data#ContentCatalog
:ContentCatalog rdf:type owl:Class ;
                rdfs:subClassOf :DataType .


###  http://www.example.org/cm/igso/2023/igso-data#ContentLifecycle
:ContentLifecycle rdf:type owl:Class ;
                  rdfs:subClassOf :LifecycleType .


###  http://www.example.org/cm/igso/2023/igso-data#ContentObject
:ContentObject rdf:type owl:Class ;
               rdfs:subClassOf :ObjectType ;
               rdfs:comment "A content object represents an electronic data object like office, PDF/A , images, audio and video files." ;
               rdfs:label "ContentObject" .


###  http://www.example.org/cm/igso/2023/igso-data#ContractorData
:ContractorData rdf:type owl:Class ;
                rdfs:subClassOf :BusinessAsset .


###  http://www.example.org/cm/igso/2023/igso-data#CustomerData
:CustomerData rdf:type owl:Class ;
              rdfs:subClassOf :BusinessAsset .


###  http://www.example.org/cm/igso/2023/igso-data#DataAsset
:DataAsset rdf:type owl:Class ;
           rdfs:subClassOf :AssetType .


###  http://www.example.org/cm/igso/2023/igso-data#DataDomain
:DataDomain rdf:type owl:Class .


###  http://www.example.org/cm/igso/2023/igso-data#DataLifecycle
:DataLifecycle rdf:type owl:Class ;
               rdfs:subClassOf :LifecycleType .


###  http://www.example.org/cm/igso/2023/igso-data#DataType
:DataType rdf:type owl:Class ;
          rdfs:subClassOf :DataDomain ;
          rdfs:label "DataType" .


###  http://www.example.org/cm/igso/2023/igso-data#Database
:Database rdf:type owl:Class ;
          rdfs:subClassOf :DataType ,
                          [ owl:intersectionOf ( dctype:Collection
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty igso-ddy:isComposedOf ;
                                                   owl:someValuesFrom dctype:Dataset
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ;
          rdfs:label "Database" .


###  http://www.example.org/cm/igso/2023/igso-data#DesignAsset
:DesignAsset rdf:type owl:Class ;
             rdfs:subClassOf :AssetType .


###  http://www.example.org/cm/igso/2023/igso-data#DisposionRecord
:DisposionRecord rdf:type owl:Class ;
                 rdfs:subClassOf :DispositionType .


###  http://www.example.org/cm/igso/2023/igso-data#DispositionPolicy
:DispositionPolicy rdf:type owl:Class ;
                   rdfs:subClassOf :Policy ;
                   owl:disjointWith :RetentionPolicy .


###  http://www.example.org/cm/igso/2023/igso-data#DispositionRule
:DispositionRule rdf:type owl:Class ;
                 rdfs:subClassOf :Rule ;
                 owl:disjointWith :RetentionRule .


###  http://www.example.org/cm/igso/2023/igso-data#DispositionSchedule
:DispositionSchedule rdf:type owl:Class ;
                     rdfs:subClassOf :DispositionType ;
                     owl:disjointWith :DispositionTrigger .


###  http://www.example.org/cm/igso/2023/igso-data#DispositionTrigger
:DispositionTrigger rdf:type owl:Class ;
                    rdfs:subClassOf :DispositionType ,
                                    :TriggerType ;
                    rdfs:label "DispositionTrigger" .


###  http://www.example.org/cm/igso/2023/igso-data#DispositionType
:DispositionType rdf:type owl:Class ;
                 rdfs:subClassOf :DataType ;
                 rdfs:label "DispositionType" .


###  http://www.example.org/cm/igso/2023/igso-data#Document
:Document rdf:type owl:Class ;
          rdfs:subClassOf :DataType ;
          rdfs:label "Document" .


###  http://www.example.org/cm/igso/2023/igso-data#FulltextIndex
:FulltextIndex rdf:type owl:Class ;
               rdfs:subClassOf :Database .


###  http://www.example.org/cm/igso/2023/igso-data#HRAsset
:HRAsset rdf:type owl:Class ;
         rdfs:subClassOf :AssetType .


###  http://www.example.org/cm/igso/2023/igso-data#HRData
:HRData rdf:type owl:Class ;
        rdfs:subClassOf :HRAsset .


###  http://www.example.org/cm/igso/2023/igso-data#HierarchcalStoreage
:HierarchcalStoreage rdf:type owl:Class ;
                     rdfs:subClassOf :StorageType .


###  http://www.example.org/cm/igso/2023/igso-data#IPAsset
:IPAsset rdf:type owl:Class ;
         rdfs:subClassOf :AssetType .


###  http://www.example.org/cm/igso/2023/igso-data#ITAsset
:ITAsset rdf:type owl:Class ;
