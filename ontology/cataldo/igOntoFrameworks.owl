    <!-- http://www.example.org/cm/igso/2023//igso-frw#Capability_igmi -->

    <owl:Class rdf:about="http://www.example.org/cm/igso/2023//igso-frw#Capability_igmi">
        <rdfs:subClassOf rdf:resource="http://www.example.org/cm/igso/2023//igso-frw#IGMI_arma"/>
        <rdfs:comment xml:lang="en">Information Access, Protection,  Privacy, Information Lifecycle ( Capture, collaboration, version control, retention, storage, holds, eDiscovery, Disposal, Archiving), Search</rdfs:comment>
