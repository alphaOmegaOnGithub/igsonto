    <owl:Class rdf:about="http://www.example.org/cm/igso/2023//igso-frw#Infrastructure_igmi">
        <rdfs:subClassOf rdf:resource="http://www.example.org/cm/igso/2023//igso-frw#IGMI_arma"/>
        <rdfs:comment xml:lang="en">Application, Software, Content Services, APIs, SLA, Licensing, Network, Connectivity, Hosting, Cloud, Servers, Information Security</rdfs:comment>
