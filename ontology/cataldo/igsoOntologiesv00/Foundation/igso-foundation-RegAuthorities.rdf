        <rdfs:label>registers</rdfs:label>
        <skos:definition>records something in a registry or archive</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/RegistrationAuthorities/specifies -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/RegistrationAuthorities/specifies">
