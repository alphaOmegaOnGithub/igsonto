    <!-- https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/RegistrationAuthorities/hasRegistrationAuthority -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/RegistrationAuthorities/hasRegistrationAuthority">
        <owl:inverseOf rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/RegistrationAuthorities/isRegistrationAuthorityFor"/>
        <rdfs:isDefinedBy rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/RegistrationAuthorities/"/>
        <rdfs:label>has registration authority</rdfs:label>
        <skos:definition>indicates the registration authority for something</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/RegistrationAuthorities/hasRegistryEntry -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/RegistrationAuthorities/hasRegistryEntry">
        <rdfs:subPropertyOf rdf:resource="https://www.omg.org/spec/Commons/Collections/comprises"/>
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/RegistrationAuthorities/Registry"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/RegistrationAuthorities/RegistryEntry"/>
        <rdfs:isDefinedBy rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/RegistrationAuthorities/"/>
        <rdfs:label>has registry entry</rdfs:label>
        <skos:definition>links a registry to entries that it contains</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/RegistrationAuthorities/isRegisteredBy -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/RegistrationAuthorities/isRegisteredBy">
        <owl:inverseOf rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/RegistrationAuthorities/registers"/>
        <rdfs:isDefinedBy rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/RegistrationAuthorities/"/>
        <rdfs:label>is registered by</rdfs:label>
        <skos:definition>indicates the registration authority or registrar that registers something</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/RegistrationAuthorities/isRegisteredIn -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/RegistrationAuthorities/isRegisteredIn">
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/RegistrationAuthorities/Registry"/>
        <rdfs:isDefinedBy rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/RegistrationAuthorities/"/>
        <rdfs:label>is registered in</rdfs:label>
        <skos:definition>indicates the registry that something is registered in</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/RegistrationAuthorities/isRegistrationAuthorityFor -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/RegistrationAuthorities/isRegistrationAuthorityFor">
        <rdfs:isDefinedBy rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/RegistrationAuthorities/"/>
        <rdfs:label>is registration authority for</rdfs:label>
        <skos:definition>indicates the thing (typically an identifier or code) that the registration authority has authority for</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/RegistrationAuthorities/registers -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/RegistrationAuthorities/registers">
