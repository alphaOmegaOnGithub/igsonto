    <!-- https://spec.edmcouncil.org/fibo/ontology/BE/LegalEntities/LegalPersons/isOrganizedIn -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/BE/LegalEntities/LegalPersons/isOrganizedIn">
        <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/ontology/BE/LegalEntities/LegalPersons/isRecognizedIn"/>
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/ontology/BE/LegalEntities/LegalPersons/LegalEntity"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Law/Jurisdiction/Jurisdiction"/>
        <rdfs:isDefinedBy rdf:resource="https://spec.edmcouncil.org/fibo/ontology/BE/LegalEntities/LegalPersons/"/>
        <rdfs:label>is organized in</rdfs:label>
        <skos:definition>indicates the jurisdiction whose laws a legal entity is organized under</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/fibo/ontology/BE/LegalEntities/LegalPersons/isRecognizedIn -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/BE/LegalEntities/LegalPersons/isRecognizedIn">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/ontology/BE/LegalEntities/LegalPersons/LegalPerson"/>
                    <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/ontology/FND/Agreements/Agreements/Agreement"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Law/Jurisdiction/Jurisdiction"/>
        <rdfs:isDefinedBy rdf:resource="https://spec.edmcouncil.org/fibo/ontology/BE/LegalEntities/LegalPersons/"/>
        <rdfs:label>is recognized in</rdfs:label>
        <skos:definition>indicates the jurisdiction in which a legal person is considered competent to enter into a contract, conduct business, or participate in other activities, or in which an agreement may be acknowledged and possibly enforceable</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/fibo/ontology/FND/Organizations/FormalOrganizations/employs -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/FND/Organizations/FormalOrganizations/employs">
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/ontology/BE/LegalEntities/LegalPersons/LegalPerson"/>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/fibo/ontology/FND/Organizations/FormalOrganizations/isEmployedBy -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/FND/Organizations/FormalOrganizations/isEmployedBy">
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/ontology/BE/LegalEntities/LegalPersons/LegalPerson"/>
    </owl:ObjectProperty>
    


