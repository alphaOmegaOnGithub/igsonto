    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/succeeds">
        <owl:equivalentProperty rdf:resource="https://www.omg.org/spec/Commons/DatesAndTimes/succeeds"/>
        <rdfs:isDefinedBy rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/"/>
        <owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:deprecated>
    </owl:ObjectProperty>
    


    <!-- https://www.omg.org/spec/Commons/DatesAndTimes/precedes -->

    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/Commons/DatesAndTimes/precedes">
