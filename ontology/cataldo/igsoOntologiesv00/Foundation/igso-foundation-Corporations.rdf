    <!-- https://spec.edmcouncil.org/fibo/ontology/BE/Corporations/Corporations/hasDateOfIncorporation -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/BE/Corporations/Corporations/hasDateOfIncorporation">
        <rdfs:subPropertyOf rdf:resource="https://www.omg.org/spec/Commons/DatesAndTimes/hasExplicitDate"/>
        <rdfs:range rdf:resource="https://www.omg.org/spec/Commons/DatesAndTimes/ExplicitDate"/>
        <rdfs:isDefinedBy rdf:resource="https://spec.edmcouncil.org/fibo/ontology/BE/Corporations/Corporations/"/>
        <rdfs:label>has date of incorporation</rdfs:label>
        <skos:definition>indicates the formal date of incorporation as stated in filing documents</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/fibo/ontology/BE/Corporations/Corporations/hasDateOfRegistration -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/BE/Corporations/Corporations/hasDateOfRegistration">
        <rdfs:subPropertyOf rdf:resource="https://www.omg.org/spec/Commons/DatesAndTimes/hasExplicitDate"/>
        <rdfs:range rdf:resource="https://www.omg.org/spec/Commons/DatesAndTimes/ExplicitDate"/>
        <rdfs:isDefinedBy rdf:resource="https://spec.edmcouncil.org/fibo/ontology/BE/Corporations/Corporations/"/>
        <rdfs:label>has date of registration</rdfs:label>
        <skos:definition>indicates the date on which the corporation has registered in some jurisdiction for regulatory and / or for tax purposes</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/fibo/ontology/BE/Corporations/Corporations/hasIssuedCapital -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/BE/Corporations/Corporations/hasIssuedCapital">
        <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/CurrencyAmount/hasMonetaryAmount"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/CurrencyAmount/MonetaryAmount"/>
        <rdfs:isDefinedBy rdf:resource="https://spec.edmcouncil.org/fibo/ontology/BE/Corporations/Corporations/"/>
        <rdfs:label>has issued capital</rdfs:label>
        <skos:definition>indicates the aggregate value of all shares held by shareholders</skos:definition>
        <cmns-av:explanatoryNote>A corporation can, at any time, issue new shares up to the full amount of authorized share capital.</cmns-av:explanatoryNote>
        <cmns-av:synonym>has subscribed capital</cmns-av:synonym>
        <cmns-av:synonym>has subscribed share capital</cmns-av:synonym>
    </owl:ObjectProperty>
    


