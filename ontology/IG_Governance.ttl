  FILTER (?inputArticle IN (igonto:Article_15, igonto:Article_25, igonto:Article_17))
}""" .


###  http://www.semanticweb.org/IG/Governance#Give_Articles_That_Include_All_Keywords
Governance:Give_Articles_That_Include_All_Keywords rdf:type owl:NamedIndividual ,
                                                            Governance:Practice_Query ;
                                                   rdfs:comment """Returns articles that inlcude all keywords the user provided.

INPUT: ?filterKeyword, (COUNT(DISTINCT ?filterKeyword) = ?)

PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX igonto: <http://www.semanticweb.org/IG/Governance#>

SELECT DISTINCT ?article
WHERE {
  ?article igonto:includesKeyword ?keyword.
  
  FILTER (
    ?keyword IN (igonto:Right, igonto:Data_Subject)
  )
  {
    SELECT ?article
    WHERE {
      ?article igonto:includesKeyword ?filterKeyword.
      FILTER (?filterKeyword IN (igonto:Right, igonto:Data_Subject))
    }
    GROUP BY ?article
    HAVING (COUNT(DISTINCT ?filterKeyword) = 2)  # Adjust this number to match the number of required keywords
  }
}""" .


