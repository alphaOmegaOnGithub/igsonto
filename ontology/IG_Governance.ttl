###  http://www.semanticweb.org/IG/Governance#Q1
Governance:Q1 rdf:type owl:NamedIndividual ,
                       Governance:Practice_Query ;
              rdfs:comment """Returns the responsible Regulation of the given Company

INPUT: ?company

PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX igonto: <http://www.semanticweb.org/IG/Governance#>


SELECT ?regulation WHERE{
  ?company igonto:operatesIn ?country.
  ?country igonto:isRegulatedBy ?regulation.
  VALUES ?company {igonto:Company_TOSCA}
}""" .


###  http://www.semanticweb.org/IG/Governance#Q2
Governance:Q2 rdf:type owl:NamedIndividual ,
                       Governance:Practice_Query ;
              rdfs:comment """Select BusinessUnit, Process, Description, Level, Risk, Consequence GroupBy BusinessUnit, Level ;

INPUT: ?organization



PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX igonto: <http://www.semanticweb.org/IG/Governance#>

SELECT * WHERE{
  ?process igonto:linkedToOrganization ?organization. 
  ?process igonto:riskDescription ?risk
}ORDER BY ?organization ?process ?risk""" .


###  http://www.semanticweb.org/IG/Governance#Q3
Governance:Q3 rdf:type owl:NamedIndividual ,
                       Governance:Practice_Query ;
              rdfs:comment """PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX igonto: <http://www.semanticweb.org/IG/Governance#>

SELECT * WHERE{
  ?capability igonto:linkedToProcess ?process.
  ?process igonto:linkedToOrganization ?organization. 
  ?capability igonto:definesDesign ?design.
  ?process igonto:riskDescription ?risk
}ORDER BY ?organization ?process ?risk ?design""" .


###  http://www.semanticweb.org/IG/Governance#Q7
Governance:Q7 rdf:type owl:NamedIndividual ,
                       Governance:Practice_Query ;
              rdfs:comment """
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX igonto: <http://www.semanticweb.org/IG/Governance#>



SELECT ?capability ?goal ?description 
       (GROUP_CONCAT(DISTINCT REPLACE(STR(?objectType), \"http://www.semanticweb.org/IG/Governance#\", \"\") ; SEPARATOR=\", \") AS ?technicalRequirement)
       (GROUP_CONCAT(DISTINCT REPLACE(STR(?ISO), \"http://www.semanticweb.org/IG/Governance#\", \"\") ; SEPARATOR=\", \") AS ?ISOs)
       (GROUP_CONCAT(DISTINCT REPLACE(STR(?articles), \"http://www.semanticweb.org/IG/Governance#\", \"\") ; SEPARATOR=\", \") AS ?GDPR_Articles)
       ?framework ?process 
       ?comment
       ?maturityLevel
       ?companyCapability
       ?requirement
       ?country
       ?regulation
WHERE {
  {
    ?annotation rdf:type owl:Axiom;
                owl:annotatedSource ?capability;
                owl:annotatedProperty igonto:needsTechnicalRequirement ;
                owl:annotatedTarget ?object;
                rdfs:comment ?comment.
    OPTIONAL { ?capability igonto:hasGoal ?goal }.
    ?capability igonto:linkedToFramework ?framework.
    OPTIONAL{?capability igonto:linkedToProcess ?process.
    ?capability igonto:capabilityDescription ?description.}
    ?capability igonto:inMaturityLevel ?maturityLevel.
    VALUES ?maturityLevel {igonto:3_Essential igonto:4_Proactive igonto:5_Transformational}
    OPTIONAL { ?object a ?objectType }.
    OPTIONAL { ?object igonto:involvesArticle ?articles }.
    OPTIONAL { ?object igonto:practicesDefinedInIso ?ISO }.
    FILTER (?objectType != owl:NamedIndividual)
  } 
  UNION
  {
    SELECT DISTINCT ?company ?companyCapability ?requirement ?country ?regulation WHERE {
      ?company igonto:operatesIn ?country.
      ?country igonto:isRegulatedBy ?regulation.
      ?company igonto:hasCompanyCapability ?companyCapability.
      ?requirement igonto:isRequiredForCompanyCapability ?companyCapability.
      VALUES ?company {igonto:Company_TOSCA}
    }
  }
} 
GROUP BY ?capability ?goal ?framework ?process ?description ?comment ?maturityLevel ?companyCapability ?requirement ?country ?regulation""" .


